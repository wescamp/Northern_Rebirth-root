#: Northern_Rebirth/scenarios/Stolen_Gold_Alone.cfg:1101
msgid " *roars in pain*"
msgstr "*Brüllt vor Schmerzen*"

#: Northern_Rebirth/scenarios/Stolen_Gold.cfg:1689
#: Northern_Rebirth/scenarios/Showdown_Alone.cfg:1217
#: Northern_Rebirth/scenarios/Eastern_Flank_Alone.cfg:844
